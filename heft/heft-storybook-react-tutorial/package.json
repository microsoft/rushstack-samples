{
  "name": "heft-storybook-react-tutorial",
  "description": "This project illustrates usage of Storybook with Heft",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "heft build --clean",
    "start": "heft build-watch",
    "storybook": "heft build-watch --storybook",
    "build-storybook": "heft build --storybook",
    "_phase:build": "heft run --only build -- --clean",
    "_phase:test": "heft run --only test -- --clean"
  },
  "dependencies": {
    "react-dom": "~16.13.1",
    "react": "~16.13.1",
    "tslib": "~2.3.1"
  },
  "devDependencies": {
    "@babel/core": "~7.20.0",
    "@rushstack/eslint-config": "^3.3.2",
    "@rushstack/heft-jest-plugin": "^0.7.13",
    "@rushstack/heft-lint-plugin": "^0.1.14",
    "@rushstack/heft-storybook-plugin": "^0.3.16",
    "@rushstack/heft-typescript-plugin": "^0.1.14",
    "@rushstack/heft-webpack4-plugin": "^0.7.5",
    "@rushstack/heft": "^0.56.2",
    "@storybook/react": "~6.4.18",
    "@types/heft-jest": "1.0.2",
    "@types/node": "14.18.36",
    "@types/react-dom": "16.9.14",
    "@types/react": "16.14.23",
    "@types/webpack-env": "1.18.0",
    "css-loader": "~5.2.7",
    "eslint": "~8.7.0",
    "heft-storybook-react-tutorial-storykit": "workspace:*",
    "html-webpack-plugin": "~4.5.2",
    "source-map-loader": "~1.1.3",
    "style-loader": "~2.0.0",
    "typescript": "~5.0.4",
    "webpack": "~4.44.2"
  }
}
